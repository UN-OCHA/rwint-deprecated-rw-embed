{
  "name": "rw-embed",
  "version": "0.2.6",
  "description": "ReliefWeb embedding service featuring oEmbed.",
  "engines": {
    "node": "0.10.x"
  },
  "main": "server.js",
  "scripts": {
    "start": "node_modules/.bin/pm2 start server.js --name 'rwembed' -e /var/log/rw-embed/err.log -o /var/log/rw-embed/out.log --merge-logs --watch --ignore-watch=\".git container logs node_modules\" --no-daemon",
    "postinstall": "cd node_modules/rw-widgets && bower install --production --allow-root && cd -"
  },
  "keywords": [
    "widget",
    "oembed",
    "embed",
    "reliefweb"
  ],
  "author": "ReliefWeb",
  "license": "proprietary",
  "private": true,
  "devDependencies": {
    "code": "^1.2.1",
    "eslint": "^0.10.0",
    "grunt": "^0.4.5",
    "grunt-cache-bust": "^0.4.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.12.2",
    "grunt-contrib-uglify": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^2.1.0",
    "grunt-lab": "^2.1.2",
    "grunt-mkdir": "^0.1.2",
    "grunt-string-replace": "^1.0.0",
    "grunt-usemin": "^3.0.0",
    "grunt-wiredep": "^2.0.0",
    "lab": "^5.1.0",
    "load-grunt-tasks": "^2.0.0",
    "nodemon": "^1.3.5",
    "time-grunt": "^1.0.0"
  },
  "dependencies": {
    "boom": "^2.6.1",
    "good": "^5.1.1",
    "good-console-json": "git://github.com/grayside/good-console-json",
    "good-file": "^4.0.2",
    "hapi": "^8.1.0",
    "hapi-named-routes": "^0.3.0",
    "hapi-oembed-provider": "^0.1.0",
    "hapi-swagger": "^0.6.2",
    "joi": "^5.1.0",
    "pm2": "^0.12.0",
    "require-directory": "^2.0.0",
    "rw-widgets": "^0.3.3"
  }
}
